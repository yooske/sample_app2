<h2>Title</h2>
<%= @list.title %>
<h2>Body</h2>
<%= @list.body %>
<h2>Image</h2>
<% if @list.image.attached? %>
  <%= image_tag @list.image, size: "50x50" %><br>
<% else %>
  <%= image_tag 'no_image', size: "50x50" %><br>
<% end %>
<%= link_to "edit", edit_list_path(@list) %><br>
<%= link_to "destroy", list_path(@list), method: :delete, "data-confirm" => "本当に削除しますか?" %>
