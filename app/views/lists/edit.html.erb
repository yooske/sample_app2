<h1>投稿編集</h1>
<% if @list.errors.any? %>
  <%= @list.errors.count %>件のエラーが発生!
  <ul>
    <% @list.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
  </ul>
<% end %>
<%= form_with model: @list, url: list_path, method: :patch do |f| %>
  <%= f.text_field :title %><br>
  <%= f.text_area :body %><br>
  <%= f.file_field :image, accept: "image/*" %>
  <%= f.submit "保存" %>
<% end %>
